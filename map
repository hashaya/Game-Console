#include<stdio.h>

char*readMapLine(char*filename){
    char c;
    char*line;
    line=(char*)malloc(12*sizeof(char));
    line=(char*)malloc(12*sizeof(char));
    int i;
    FILE*stream;
    stream=fopen(filename,"r");
    for(i=0;(c=getc(stream))!='\n';i++)
        *(line+i)=c;
    fclose(stream);
    *(line+i)='\0';
    return line;
}

int*getMapDimensions(char*str){
    int*res;
    int x=0,y=0,i;
    char c;
    for(i=0;(c=*(str+i))!='x';i++){
        x=x*10+(c-'0');
    }
    for(i++;(c=*(str+i))!='\0';i++){
        y=y*10+(c-'0');
    }
    *res=x;
    *(res+1)=y;
    return res;
}
